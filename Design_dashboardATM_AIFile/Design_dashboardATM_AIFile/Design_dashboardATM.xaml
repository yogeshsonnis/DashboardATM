                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                                  SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}"
                                                  VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />

                                <Path x:Name="path"
                                      Grid.Column="1"
                                      Data="F1M5.832,10.704L4.986,9.934C1.982,7.21 -0.001,5.414 -0.001,3.208 -0.001,1.412 1.41,0 3.207,0 4.222,0 5.196,0.473 5.832,1.219 6.468,0.473 7.442,0 8.457,0 10.254,0 11.666,1.412 11.666,3.208 11.666,5.414 9.682,7.21 6.678,9.94z"
